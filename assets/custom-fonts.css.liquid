/* light font-weight */
@font-face {
  font-family: "Neue Haas Grotesk";
  src: url({{ 'NeueHaasDisplay-Light.woff2' | file_url }}) format('woff2');
  font-style: normal;
  font-weight: 100;
  font-display: swap;
}

/* regular font-weight */
@font-face {
  font-family: "Neue Haas Unica";
  src: url({{ 'NeueHaasUnica-Regular.woff2' | file_url }}) format('woff2');
  font-style: normal;
  font-weight: 200;
  font-display: swap;
}

/* medium font-weight */
@font-face {
  font-family: "Neue Haas Grotesk";
  src: url({{ 'NeueHaasDisplay-Mediu.woff2' | file_url }}) format('woff2');
  font-style: normal;
  font-weight: 300;
  font-display: swap;
}
